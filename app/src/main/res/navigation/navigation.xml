<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/scannerFragment"
        android:name="com.puntogris.blint.ui.ScannerFragment"
        android:label="Escanear codigo" >
        <argument
            android:name="originDestination"
            app:argType="integer"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_scannerFragment_to_productFragment"
            app:destination="@id/productFragment" />
        <action
            android:id="@+id/action_scannerFragment_to_editProductFragment"
            app:destination="@id/editProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.puntogris.blint.ui.main.MainFragment"
        android:label="@string/app_name" >
        <action
            android:id="@+id/action_mainFragment_to_registerBusiness"
            app:destination="@id/registerBusinessFragment" />
        <action
            android:id="@+id/action_mainFragment_to_eventInfoBottomSheet"
            app:destination="@id/eventInfoBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/registerBusinessFragment"
        android:name="com.puntogris.blint.ui.business.RegisterBusinessFragment"
        android:label="Registrar Negocio" >
        <action
            android:id="@+id/action_registerBusinessFragment_to_registerOnlineBusinessFragment"
            app:destination="@id/registerOnlineBusinessFragment" />
        <action
            android:id="@+id/action_registerBusinessFragment_to_registerLocalBusinessFragment"
            app:destination="@id/registerLocalBusinessFragment" />
    </fragment>
    <fragment
        android:id="@+id/preferencesFragment"
        android:name="com.puntogris.blint.ui.settings.PreferencesFragment"
        android:label="Configuracion" />
    <fragment
        android:id="@+id/clientFragment"
        android:name="com.puntogris.blint.ui.client.ClientFragment"
        android:label="Cliente" >
        <argument
            android:name="clientID"
            app:argType="integer" />
        <action
            android:id="@+id/action_clientFragment_to_editClientFragment"
            app:destination="@id/editClientFragment" />
        <action
            android:id="@+id/action_clientFragment_to_recordInfoBottomSheet"
            app:destination="@id/recordInfoBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/supplierFragment"
        android:name="com.puntogris.blint.ui.supplier.SupplierFragment"
        android:label="Proveedor" >
        <argument
            android:name="supplierID"
            app:argType="integer" />
        <action
            android:id="@+id/action_supplierFragment_to_editSupplierFragment"
            app:destination="@id/editSupplierFragment" />
        <action
            android:id="@+id/action_supplierFragment_to_recordInfoBottomSheet"
            app:destination="@id/recordInfoBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/manageProductsFragment"
        android:name="com.puntogris.blint.ui.product.ManageProductsFragment"
        android:label="Productos" >
        <action
            android:id="@+id/action_manageProductsFragment_to_productFragment"
            app:destination="@id/productFragment" />
    </fragment>
    <fragment
        android:id="@+id/manageSuppliersFragment"
        android:name="com.puntogris.blint.ui.supplier.ManageSuppliersFragment"
        android:label="Proveedores" >
        <action
            android:id="@+id/action_manageSuppliersFragment_to_supplierFragment"
            app:destination="@id/supplierFragment" />
    </fragment>
    <fragment
        android:id="@+id/manageClientsFragment"
        android:name="com.puntogris.blint.ui.client.ManageClientsFragment"
        android:label="Clientes" >
        <action
            android:id="@+id/action_manageClientsFragment_to_clientFragment"
            app:destination="@id/clientFragment" />
    </fragment>
    <fragment
        android:id="@+id/productFragment"
        android:name="com.puntogris.blint.ui.product.ProductFragment"
        android:label="Producto" >
        <argument
            android:name="productID"
            app:argType="integer" />
        <action
            android:id="@+id/action_productFragment_to_editProductFragment"
            app:destination="@id/editProductFragment" />
        <action
            android:id="@+id/action_productFragment_to_createRecordFragment"
            app:destination="@id/createRecordFragment" />
        <action
            android:id="@+id/action_productFragment_to_recordInfoBottomSheet"
            app:destination="@id/recordInfoBottomSheet" />
        <action
            android:id="@+id/action_productFragment_to_supplierFragment"
            app:destination="@id/supplierFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.puntogris.blint.ui.login.LoginFragment"
        android:label="" >
        <action
            android:id="@+id/action_loginFragment_to_loginProblemsFragment"
            app:destination="@id/loginProblemsFragment" />
        <action
            android:id="@+id/action_loginFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginProblemsFragment"
        android:name="com.puntogris.blint.ui.login.LoginProblemsFragment"
        android:label="Ayuda" />
    <fragment
        android:id="@+id/registerLocalBusinessFragment"
        android:name="com.puntogris.blint.ui.business.RegisterLocalBusinessFragment"
        android:label="Registrar negocio local" />
    <fragment
        android:id="@+id/registerOnlineBusinessFragment"
        android:name="com.puntogris.blint.ui.business.RegisterOnlineBusinessFragment"
        android:label="Registrar negocio online" />
    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.puntogris.blint.ui.calendar.CalendarFragment"
        android:label="Calendario" >
        <action
            android:id="@+id/action_calendarFragment_to_eventInfoBottomSheet"
            app:destination="@id/eventInfoBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.puntogris.blint.ui.about.AboutFragment"
        android:label="Sobre Blint" >
        <action
            android:id="@+id/action_aboutFragment_to_termsConditionsFragment"
            app:destination="@id/termsConditionsFragment" />
        <action
            android:id="@+id/action_aboutFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment" />
    </fragment>
    <fragment
        android:id="@+id/termsConditionsFragment"
        android:name="com.puntogris.blint.ui.about.TermsConditionsFragment"
        android:label="Terminos y condiciones" />
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.puntogris.blint.ui.about.PrivacyPolicyFragment"
        android:label="Politica de privacidad" />
    <fragment
        android:id="@+id/editProductFragment"
        android:name="com.puntogris.blint.ui.product.EditProductFragment"
        android:label="Producto" >
        <argument
            android:name="productID"
            app:argType="integer"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_editProductFragment_to_scannerFragment"
            app:destination="@id/scannerFragment" />
        <argument
            android:name="barcodeScanned"
            app:argType="string"
            android:defaultValue=" " />
    </fragment>
    <fragment
        android:id="@+id/editClientFragment"
        android:name="com.puntogris.blint.ui.client.EditClientFragment"
        android:label="Cliente" >
        <argument
            android:name="clientID"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/editSupplierFragment"
        android:name="com.puntogris.blint.ui.supplier.EditSupplierFragment"
        android:label="Proveedor" >
        <argument
            android:name="supplierID"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/joinBusinessFragment"
        android:name="com.puntogris.blint.ui.business.JoinBusinessFragment"
        android:label="" >
        <action
            android:id="@+id/action_joinBusinessFragment_to_scannerFragment"
            app:destination="@id/scannerFragment" />
    </fragment>
    <dialog
        android:id="@+id/scannerResultDialog"
        android:name="com.puntogris.blint.ui.ScannerResultBottomSheet"
        android:label="" />
    <fragment
        android:id="@+id/accountPreferences"
        android:name="com.puntogris.blint.ui.settings.AccountPreferences"
        android:label="Mi cuenta" />
    <fragment
        android:id="@+id/backUpPreferences"
        android:name="com.puntogris.blint.ui.settings.BackupPreferences"
        android:label="Copias de seguridad" />
    <fragment
        android:id="@+id/notificationsPreferences"
        android:name="com.puntogris.blint.ui.settings.NotificationsPreferences"
        android:label="Notificaciones" />
    <fragment
        android:id="@+id/userAccountFragment"
        android:name="com.puntogris.blint.ui.account.UserAccountFragment"
        android:label="Mi datos" />
    <fragment
        android:id="@+id/restoreBackupFragment"
        android:name="com.puntogris.blint.ui.backup.RestoreBackupFragment"
        android:label="Restaurar negocio" />
    <fragment
        android:id="@+id/createBackupFragment"
        android:name="com.puntogris.blint.ui.backup.CreateBackupFragment"
        android:label="Respaldar negocio" />
    <fragment
        android:id="@+id/manageRecordsFragment"
        android:name="com.puntogris.blint.ui.record.ManageRecordsFragment"
        android:label="Movimientos" >
        <action
            android:id="@+id/action_manageRecordsFragment_to_recordInfoBottomSheet"
            app:destination="@id/recordInfoBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/createRecordFragment"
        android:name="com.puntogris.blint.ui.record.CreateRecordFragment"
        android:label="Nuevo movimiento" >
        <argument
            android:name="productID"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <dialog
        android:id="@+id/recordInfoBottomSheet"
        android:name="com.puntogris.blint.ui.record.RecordInfoBottomSheet"
        android:label="RecordInfoBottomSheet" >
        <argument
            android:name="record"
            app:argType="com.puntogris.blint.model.Record" />
        <action
            android:id="@+id/action_recordInfoBottomSheet_to_supplierFragment"
            app:destination="@id/supplierFragment" />
        <action
            android:id="@+id/action_recordInfoBottomSheet_to_clientFragment"
            app:destination="@id/clientFragment" />
    </dialog>
    <fragment
        android:id="@+id/operationsFragment"
        android:name="com.puntogris.blint.ui.operations.OperationsFragment"
        android:label="Herramientas" />

    <fragment
        android:id="@+id/generalPriceChangeFragment"
        android:name="com.puntogris.blint.ui.operations.GeneralPriceChangeFragment"
        android:label="Edicion general" />
    <fragment
        android:id="@+id/importProductsFragment"
        android:name="com.puntogris.blint.ui.operations.ImportProductsFragment"
        android:label="Importar lista externa" />
    <fragment
        android:id="@+id/reportsFragment"
        android:name="com.puntogris.blint.ui.reports.ReportsFragment"
        android:label="Informes" >
        <action
            android:id="@+id/action_reportsFragment_to_generateReportFragment"
            app:destination="@id/generateReportFragment" />
    </fragment>
    <fragment
        android:id="@+id/createEventFragment"
        android:name="com.puntogris.blint.ui.calendar.CreateEventFragment"
        android:label="Nuevo evento" />
    <dialog
        android:id="@+id/eventInfoBottomSheet"
        android:name="com.puntogris.blint.ui.calendar.EventInfoBottomSheet"
        android:label="EventInfoBottomSheet" >
        <argument
            android:name="event"
            app:argType="com.puntogris.blint.model.Event" />
    </dialog>
    <fragment
        android:id="@+id/generateReportFragment"
        android:name="com.puntogris.blint.ui.reports.GenerateReportFragment"
        android:label="Generar informe" >
        <argument
            android:name="reportCode"
            app:argType="integer" />
        <argument android:name="timeCode"
            app:argType="string"
            android:defaultValue=" " />
        <argument
            android:name="startTime"
            app:argType="long"
            android:defaultValue="0L" />
        <argument
            android:name="endTime"
            app:argType="long"
            android:defaultValue="0L" />
    </fragment>
    <fragment
        android:id="@+id/manageCategoriesFragment"
        android:name="com.puntogris.blint.ui.categories.ManageCategoriesFragment"
        android:label="Categorias" />
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.puntogris.blint.ui.login.WelcomeFragment"
        android:label="" >
    </fragment>
    <fragment
        android:id="@+id/introFragment"
        android:name="com.puntogris.blint.ui.login.IntroFragment"
        android:label="" >
        <action
            android:id="@+id/action_introFragment_to_firstSyncFragment"
            app:destination="@id/firstSyncFragment" />
    </fragment>
    <fragment
        android:id="@+id/firstSyncFragment"
        android:name="com.puntogris.blint.ui.login.FirstSyncFragment"
        android:label="" >
        <argument
            android:name="username"
            app:argType="string" />
        <argument
            android:name="userCountry"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/manageBusinessFragment"
        android:name="com.puntogris.blint.ui.business.ManageBusinessFragment"
        android:label="Mis negocios" >
        <action
            android:id="@+id/action_manageBusinessFragment_to_businessFragment"
            app:destination="@id/businessFragment" />
    </fragment>
    <fragment
        android:id="@+id/businessFragment"
        android:name="com.puntogris.blint.ui.business.BusinessFragment"
        android:label="Mi negocio" >
        <argument
            android:name="employee"
            app:argType="com.puntogris.blint.model.Employee" />
        <action
            android:id="@+id/action_businessFragment_to_employeeFragment"
            app:destination="@id/employeeFragment" />
        <action
            android:id="@+id/action_businessFragment_to_deleteBusinessFragment"
            app:destination="@id/deleteBusinessFragment" />
        <action
            android:id="@+id/action_businessFragment_to_addBusinessEmployee"
            app:destination="@id/addBusinessEmployee" />
    </fragment>
    <fragment
        android:id="@+id/employeeFragment"
        android:name="com.puntogris.blint.ui.business.EmployeeFragment"
        android:label="Empleado" >
        <argument
            android:name="employee"
            app:argType="com.puntogris.blint.model.Employee" />
    </fragment>
    <fragment
        android:id="@+id/deleteBusinessFragment"
        android:name="com.puntogris.blint.ui.business.DeleteBusinessFragment"
        android:label="Eliminar negocio" >
        <argument
            android:name="business"
            app:argType="com.puntogris.blint.model.Employee" />
    </fragment>
    <fragment
        android:id="@+id/addBusinessEmployee"
        android:name="com.puntogris.blint.ui.business.AddBusinessEmployee"
        android:label="Nuevo empleado" >
        <argument
            android:name="business"
            app:argType="com.puntogris.blint.model.Employee" />
    </fragment>
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.puntogris.blint.ui.notifications.NotificationsFragment"
        android:label="Notificaciones" >
        <action
            android:id="@+id/action_notificationsFragment_to_employmentRequestFragment"
            app:destination="@id/employmentRequestFragment" />
        <argument
            android:name="requestNotification"
            app:argType="com.puntogris.blint.model.notifications.EmploymentRequestReceivedNotif" />
    </fragment>
    <fragment
        android:id="@+id/employmentRequestFragment"
        android:name="com.puntogris.blint.ui.business.EmploymentRequestFragment"
        android:label="Solicitud de empleo" />
    <fragment
        android:id="@+id/newUserFragment"
        android:name="com.puntogris.blint.ui.main.NewUserFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_new_user" />
</navigation>