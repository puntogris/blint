            app:destination="@id/productFragment" />
        <action
            android:id="@+id/action_scannerFragment_to_editProductFragment"
            app:destination="@id/editProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.puntogris.blint.ui.main.MainFragment"
        android:label="@string/app_name" >
        <action
            android:id="@+id/action_mainFragment_to_registerBusiness"
            app:destination="@id/registerBusinessFragment" />
        <action
            android:id="@+id/action_mainFragment_to_eventInfoBottomSheet"
            app:destination="@id/eventInfoBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/registerBusinessFragment"
        android:name="com.puntogris.blint.ui.business.RegisterBusinessFragment"
        android:label="Registrar Negocio" >
        <action
            android:id="@+id/action_registerBusinessFragment_to_registerOnlineBusinessFragment"
            app:destination="@id/registerOnlineBusinessFragment" />
        <action
            android:id="@+id/action_registerBusinessFragment_to_registerLocalBusinessFragment"
            app:destination="@id/registerLocalBusinessFragment" />
    </fragment>
    <fragment
        android:id="@+id/preferencesFragment"
        android:name="com.puntogris.blint.ui.settings.PreferencesFragment"
        android:label="Configuracion" />
    <fragment
