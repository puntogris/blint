<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomButtonIconArrows">
        <attr name="startOpen" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RallyLineGraphChart">
        <attr name="barColor" format="color"/>
        <attr name="curveBorderColor" format="color"/>
        <attr name="curveFillColor" format="color"/>
        <attr name="curveTopMargin" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RallyScrollableTab">
        <attr name="selectedColor" format="color"/>
        <attr name="unSelectedColor" format="color"/>
        <attr name="tabTextAppearance" format="reference"/>
    </declare-styleable>

    <attr name="bottomSheetDayNight" format="color" />

    <!--A variant of primarySurface which corresponds to colorPrimaryVariant in light theme-->
    <!--and colorSurface in dark theme-->
    <attr name="colorPrimarySurfaceVariant" format="color" />

    <!--Emphasis values are used in combination with each "on" color to create three available -->
    <!--levels of emphasis for text and content.-->
    <attr name="emphasisHighAlpha" format="float"/>
    <attr name="emphasisMediumAlpha" format="float"/>
    <attr name="emphasisDisabledAlpha" format="float"/>

    <attr name="motionInterpolatorPersistent" format="reference"/>
    <attr name="motionInterpolatorIncoming" format="reference"/>
    <attr name="motionInterpolatorOutgoing" format="reference"/>

    <!--Custom view themes/styles-->
    <attr name="bottomNavigationDrawerStyle" format="reference" />
    <attr name="bottomNavigationDrawerNavigationViewTheme" format="reference" />

    <declare-styleable name="ConstraintRadioGroup">
        <!-- The id of the child radio button that should be checked by default
             within this radio group. -->
        <attr name="checkedButton2" format="integer" />
        <!--<attr name="radioButtonStyle" format="reference"/>
        <attr name="android:checkedButton"/>-->
    </declare-styleable>
</resources>